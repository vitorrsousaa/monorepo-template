{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "ci:lint": {
      "dependsOn": [
        "^ci:lint"
      ]
    },
    "format": {},
    "ci:format": {},
    "typecheck": {},
    "ci:typecheck": {},
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "ci:test": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}